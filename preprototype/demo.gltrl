:= block_list_literal […]—
   "if"
   "this"
   "works,"
   "then"

:= block_dictionary_literal {…}—
   "close issue #" 1;
   "open another issue, numbered" 2;

for |i (range 0 4)|—
   (println (_ block_list_literal i))

:= key "close issue #"
(println key)
(println (_ block_dictionary_literal key))
